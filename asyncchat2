<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00DRO000000AjrB',
				'web1',
				'https://asyncchat2.test1.pc-rnd.force.com/ESWweb11665056218092',
				{
					scrt2URL: 'https://ise3.test1.my.pc-rnd.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://asyncchat2.test1.pc-rnd.force.com/ESWweb11665056218092/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>


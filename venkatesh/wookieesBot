	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en'; // For example, enter 'en' or 'en-US'
			embeddedservice_bootstrap.settings.debugMode = true;
			embeddedservice_bootstrap.init(
				'00DSB0000002A9l',
				'Embedded_Service_deployment_BOT_GV_2',
				'https://gvinapporg1.test1.my.pc-rnd.salesforce.com',
				{
					scrt2URL: 'https://gvinapporg1.test1.my.pc-rnd.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://gvinapporg1.test1.lightning.pc-rnd.force.com/embeddedservice/asyncclient/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>


